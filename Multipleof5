package Arr;

import java.util.Scanner;

public class Multipleof5 {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);

		int[] arr = new int[5];
		System.out.println("Enter 5 numbers:");

		for (int i = 0; i < 5; i++) {
			arr[i] = sc.nextInt();
		}

		int count = 0;
		for (int x : arr) {
			if (x % 5 == 0) {
				count++;
			}
		}

		System.out.println("Count of multiples of 5 = " + count);
	}

}
